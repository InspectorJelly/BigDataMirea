      ],
      "source": [
        "import numpy as np\n",
        "a, b, h, r = 1, 1, 1, 1\n",
        "def triangle(a, h):\n",
        "    print(\"Введите основание и высоту треугольника\")\n",
        "    a = int(input())\n",
        "    h = int(input())\n",
        "    return (a * h / 2)\n",
        "\n",
        "def rectangle(a, b):\n",
        "    print(\"Введите стороны прямоугольника\")\n",
        "    a = int(input())\n",
        "    b = int(input())\n",
        "    return (a * b)\n",
        "\n",
        "def circle(r):\n",
        "    print(\"Введите радиус круга\")\n",
        "    r = int(input())\n",
        "    return (r ** 2 * np.pi)\n",
        "\n",
        "dict = {\"Треугольник\" : triangle(a, h), \"Прямоугольник\" : rectangle(a, b), \"Круг\" : circle(r)}\n",
        "print(dict)"
      ]
